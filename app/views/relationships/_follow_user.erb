<H1>Follow User</H1>
<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
</thead>
<tbody>
  <% if users.exists? %>
  <% users.each do |user| %>
    <tr>
      <th><%= image_tag user.get_profile_image, size: '50x50' %></th>
      <th><%= user.name %></th>
      <th> フォロー数:<%= user.followings.count %></th>
      <th> フォロワー数:<%= user.followers.count %></th>
      <th><%= link_to 'Show', user %></th>
    </tr>
</tbody>
<% end %>

<% else %>
<p>ユーザーはいません</p>
<% end %>
</table>